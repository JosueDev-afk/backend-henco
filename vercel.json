{
    "builds": [
      {
        "src": "angular.json",
        "use": "@vercel/static-build",
        "config": {
          "buildCommand": "ng build --prod",
          "outputDirectory": "dist/my-dashboard"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "env": {
      "MONGODB_URI": "@my-mongodb-uri"
    }
  }
  